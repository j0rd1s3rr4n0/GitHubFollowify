{
    "manifest_version": 3,
     "name": "__MSG_extName__",
     "description": "__MSG_description__",
    "version": "1.0.2",
    "permissions": [
        "activeTab"
    ],
    "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
},

    "content_scripts": [
	    {
	        "matches": [
	            "https://github.com/*",
	            "https://*.github.com/*"
	        ],
	        "js": ["content.js"]
	    }
	],
	"default_locale": "en"
}
